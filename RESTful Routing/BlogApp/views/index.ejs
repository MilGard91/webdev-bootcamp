<h1>INDEX PAGE</h1>

<% blogs.forEach(blog => { %>
    <div>
        <h2>
            <%= blog.title %>
        </h2>
        <img src="<%= blog.image %>" alt="<%= blog.title %>">
        <span>
            <%= blog.created %>
        </span>
        <p>
            <%= blog.body %>
        </p>
    </div>
    <% }) %>